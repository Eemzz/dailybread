<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dailybread">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="DailyBread"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DailyBread"
        tools:targetApi="31">
        <activity
            android:name="com.example.dailybread.SignUpActivity"
            android:exported="true"
            android:label="DailyBread SignUp"
            android:theme="@style/Theme.DailyBread">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
        android:name="com.example.dailybread.HomeActivity"
        android:exported="true"
        android:label="DailyBread Home"
        android:theme="@style/Theme.DailyBread">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.HOME" />
        </intent-filter>
    </activity>
        <activity
            android:name="com.example.dailybread.InventoryActivity"
            android:exported="true"
            android:label="DailyBread Inventory"
            android:theme="@style/Theme.DailyBread"/>
        <activity
            android:name="com.example.dailybread.EditInventoryActivity"
            android:exported="true"
            android:label="DailyBread Inventory"
            android:theme="@style/Theme.DailyBread"/>
    </application>

</manifest>